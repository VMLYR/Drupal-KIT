{% embed "@%theme_prefix%-atoms/00-field/_field.twig" with {
  label: label,
  label_hidden: label_hidden,
  field_name: field_name,
  field_type: field_type,
  bem_field_modifiers: bem_field_modifiers,
  bem_field_extra: bem_field_extra,
  attributes: attributes,
} %}

  {% block field_content %}

    {% include "@%theme_prefix%-atoms/02-text/00-headings/_heading.twig" with {
     'heading_display':heading_display,
     'heading_level':heading_level,
     'heading':heading,
     'heading_id':heading_id,
     'heading_url':heading_url,
     'bem_heading_block': false,
     'bem_heading_element': false,
    }
  %}

  {% endblock %}

{% endembed %}
